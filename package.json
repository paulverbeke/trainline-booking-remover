{
  "name": "booking.com-checkbox-remover",
  "version": "1.0.2",
  "description": "A browser extension to uncheck and remove the booking.com checkbox on several travel booking websites and prevent it from opening a new tab.",
  "scripts": {
    "build:update-version": "node scripts/update-version.js",
    "build:package": "web-ext build --source-dir=src --overwrite-dest --filename {name}-{version}.zip",
    "build": "npm-run-all build:update-version build:package",
    "validate": "web-ext lint --source-dir=src",
    "lint": "eslint src scripts --fix"
  },
  "keywords": [
    "browser-extension",
    "booking.com",
    "trainline",
    "flixbus",
    "userscript"
  ],
  "author": "Paul Verbeke",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "web-ext": "^8.3.0"
  }
}
